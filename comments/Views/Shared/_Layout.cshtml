
<!DOCTYPE html>
<html>
<head>

    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title State of Alaska Public Comments</title>
    @*@Styles.Render("~/Content/css")*@
    <link rel="stylesheet" href="~/Content/Styles.css">
    @*@Scripts.Render("~/bundles/modernizr")*@

</head>
<body>
    <!-- Hidden Skip Link for Screen Reader -->
    <a href="#content" class="sr-only">Skip to content</a>
    <header>

        <div id="HeaderRoundedCornersMask">
            <nav id="soa-nav" class="navbar navbar-default" role="navigation">
                <div class="container-fluid">
                    <!-- Brand and toggle get grouped for better mobile display -->
                    <div class="navbar-header">
                        <button class="navbar-toggle collapsed soa-collapsed-nav-btn" type="button" data-toggle="collapse" data-target="#soa-navbar-collapse">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar navbar-icon-bar"></span>
                            <span class="icon-bar navbar-icon-bar"></span>
                            <span class="icon-bar navbar-icon-bar"></span>
                        </button>
                        <a id="soa" class="navbar-brand AKImage" href="http://alaska.gov/"><span class="sr-only">State of Alaska</span></a>
                        <!-- This is the left hand side SOA button in the top bar -->
                    </div>
                    <!-- Collect the nav links, forms, and other content for toggling -->
                    <div class="collapse navbar-collapse" id="soa-navbar-collapse">
                        <ul class="nav navbar-nav">
                            <li><a href="https://myalaska.state.ak.us/home/app">myAlaska</a></li>
                            <li><a href="http://alaska.gov/akdir1.html">My Government</a></li>
                            <li><a href="http://alaska.gov/residentHome.html">Resident</a></li>
                            <li><a href="http://alaska.gov/businessHome.html">Business in Alaska</a></li>
                            <li><a href="http://alaska.gov/visitorHome.html">Visiting Alaska</a></li>
                            <li><a href="http://alaska.gov/employeeHome.html">State Employees</a></li>
                        </ul>
                    </div><!-- /.navbar-collapse -->
                </div><!-- /.container-fluid -->
            </nav>
        </div>
        <!--End SOA Header-->
        <!-- This is the start of the header div this holds the whole header banner -->
        <div class="header">
            <div id="headerInsideDiv">
                <div id="AgencyLogo" class="pull-left"><a href="http://dnr.alaska.gov" id="title"><img id="agencyLogoImg" src="/comments/Content/Images/Dmlw_header.png" alt="Alaska Department of Natural Resources Division of Mining, Land, and Water"></a></div>
                <div id="search">
                    <form action="https://google.state.ak.us/search" method="get" name="gs" class="form-inline" role="form">
                        <div class="form-group" id="searchBoxGrp">
                            <div class="input-group">
                                <div class="input-group-addon"><span class="glyphicon glyphicon-search"></span></div>
                                <input id="searchBox" class="form-control input-sm" type="search" name="q" />
                            </div>
                        </div>
                        <button id="searchButton1" type="submit" class="btn btn-sm">Search</button>

                        <div class="form-group" id="radioGrp">
                            <div class="radio">
                                <label>
                                    <input type="radio" id="agency" name="msize" value="DNR" checked="checked" />
                                    Department Of Natural Resources
                                </label>
                            </div>
                            <div class="radio">
                                <label>
                                    <input type="radio" id="entire" name="msize" value="StateWide" />
                                    State of Alaska
                                </label>
                            </div>
                        </div>
                        <div class="form-group" id="searchButton2Grp"><button id="searchButton2" type="submit" class="btn btn-sm">Search</button></div>
                        <input type="hidden" value="DNR" name="site">
                        <input type="hidden" value="DNR" name="client">
                        <input type="hidden" value="DNR" name="proxystylesheet">
                        <input type="hidden" value="xml_no_dtd" name="output">
                        <input type="hidden" name="sort" value="date:D:L:d1">
                        <input type="hidden" name="ie" value="UTF-8">
                        <input type="hidden" name="oe" value="UTF-8">
                    </form>
                </div>
                <!-- End Search -->
                <div class="clear"></div><!-- Clear the float so it doesn't affect the below content -->
            </div>
        </div>
    </header>
    <div id="application_layout" class="container">
        <div id="breadcrumbs" style="float:left">
            <!-- Breadcrumb trail, default color is gray -->
            <!-- NOTE: DOG & DGGS (and those with their own servers) - you will need to change any #echo var="SERVER_NAME" coding to the appropriate web address throughout the templates -->
            <span style="padding-top:3px;"><a href="http://dnr.alaska.gov/">Natural Resources</a>  >  <a href="http://dnr.alaska.gov/mlw">Mining, Land & Water</a> ></span>
            <div class="clear"></div><!-- Clear the float so it doesn't affect the below content -->
        </div>
        <noscript>
            <style type="text/css">
                #content {
                    display: none;
                }
            </style>
            <div class="alert alert-warning center-block text-center" style="width:75%; padding-top:15px; margin-top:40px;">For full functionality of this page it is necessary to enable JavaScript.<br />Here are the <a href="http://www.enable-javascript.com" target="_blank"> instructions how to enable JavaScript in your web browser</a><br />Be sure to refresh this page when you are done!</div>
        </noscript>
        <div id="content" class="container" style="max-width:inherit; padding:30px">


            @RenderBody()
            <hr />
        </div>
    </div>
    <footer>
        <div id="department_footer">
            <div class="footer_left">
                <ul class="inline_items">
                    <li><a href="http://dnr.alaska.gov/shared/notices/privacy.htm">Privacy</a></li>
                    <li><a href="http://dnr.alaska.gov/shared/notices/copyright.htm">Copyright</a></li>
                    <li><a href="http://dnr.alaska.gov/shared/status/systemstatus.cfm">System Status</a></li>
                    <li><a href="http://dnr.alaska.gov/shared/emailforms/emailcontact.cfm?send=dnr.webmaster">Webmaster</a></li>
                </ul>
            </div>
            <div class="footer_right">
                Department of Natural Resources<br />
                550 W. 7th Ave, Suite 1260, Anchorage, AK 99501-3557
                <ul class="separated_items">
                    <li><span>Phone: 907-269-8400</span></li>
                    <li><span>Fax: 907-269-8901</span></li>
                    <li><span>TTY: 907-269-8411</span></li>
                </ul>
            </div>
        </div>
        <div id="state_footer">
            <div class="footer_left">
                <ul class="inline_items">
                    <li><a href="http://alaska.gov">State of Alaska</a></li>
                    <li><a href="https://myalaska.state.ak.us/home/app">myAlaska</a></li>
                    <li><a href="http://alaska.gov/akdir1.html">My Government</a></li>
                    <li><a href="http://alaska.gov/residentHome.html">Resident</a></li>
                    <li><a href="http://alaska.gov/businessHome.html">Business in Alaska</a></li>
                    <li><a href="http://alaska.gov/visitorHome.html">Visiting Alaska</a></li>
                    <li><a href="http://alaska.gov/employeeHome.html">State Employees</a></li>
                </ul>
            </div>
            <div class="footer_right">
                <ul class="separated_items">
                    <li><span>State of Alaska</span></li>
                    <li><span>&copy;2017</span></li>
                    <li><span><a href="http://dnr.alaska.gov/shared/emailforms/emailcontact.cfm?send=dnr.dmlwweb" target="_blank">Webmaster</a></span></li>
                </ul>
            </div>
        </div>
        <div id="footer_padding" class="footer_decoration">&nbsp;</div>
    </footer>


    @*@Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)*@

</body>
</html>
